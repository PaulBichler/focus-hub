{
    "manifest_version": 3,
    "name": "Focus Hub",
    "description": "An extension designed to help you stay focused and productive while browsing the internet.",
    "version": "1.0",
    "icons": {
      "16": "src/images/icon/icon-16.png",
      "32": "src/images/icon/icon-32.png",
      "48": "src/images/icon/icon-48.png",
      "128": "src/images/icon/icon-128.png"
    },
    "action": {
        "default_popup": "src/popup/popup.html",
        "default_icon": {
          "16": "src/images/icon/icon-16.png",
          "32": "src/images/icon/icon-32.png",
          "48": "src/images/icon/icon-48.png",
          "128": "src/images/icon/icon-128.png"
        }
    },
    "minimum_chrome_version": "92",
    "background": {
        "service_worker": "src/service_worker/background.js",
        "type": "module"
    },
    "permissions": ["tabs", "storage"]
  }